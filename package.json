{
  "name": "same-origin",
  "version": "0.1.0",
  "description": "Checks if two URLs pass the \"same origin\"-policy (RFC 6454)",
  "main": "index.js",
  "scripts": {
    "test": "node test/tests.js|faucet",
    "build": "browserify -g [uglifyify] index.js > dist/same-origin.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rexxars/same-origin.git"
  },
  "keywords": [
    "same-origin",
    "origin",
    "sop",
    "rfc6454",
    "url"
  ],
  "browser": {
    "url": "./url-parser.js"
  },
  "author": "Espen Hovlandsdal <espen@hovlandsdal.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rexxars/same-origin/issues"
  },
  "homepage": "https://github.com/rexxars/same-origin",
  "devDependencies": {
    "browserify": "^4.2.3",
    "faucet": "^0.0.1",
    "tape": "^2.13.4",
    "uglifyify": "^2.5.0"
  },
  "testling": {
    "files": "test/*.test.js",
    "browsers": [
      "ie/8..latest",
      "firefox/17..latest",
      "firefox/nightly",
      "chrome/22..latest",
      "chrome/canary",
      "opera/12..latest",
      "opera/next",
      "safari/5.1..latest",
      "ipad/6.0..latest",
      "iphone/6.0..latest",
      "android-browser/4.2..latest"
    ]
  }
}
